{
  "name": "post-content-shortcodes",
  "version": "1.0.0",
  "description": "**Contributors:** cgrymala",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o dist/ten321/post-content-shortcodes/blocks/content src/ten321/post-content-shortcodes/blocks/content/editor.scss && node-sass -o dist/ten321/post-content-shortcodes/blocks/content src/ten321/post-content-shortcodes/blocks/content/style.scss && node-sass -o dist/ten321/post-content-shortcodes/blocks/list src/ten321/post-content-shortcodes/blocks/list/editor.scss && node-sass -o dist/ten321/post-content-shortcodes/blocks/list src/ten321/post-content-shortcodes/blocks/list/style.scss",
    "cssmin": "cleancss -o dist/ten321/post-content-shortcodes/blocks/content/block.editor.build.css dist/ten321/post-content-shortcodes/blocks/content/editor.css && cleancss -o dist/ten321/post-content-shortcodes/blocks/content/block.build.css dist/ten321/post-content-shortcodes/blocks/content/style.css && cleancss -o dist/ten321/post-content-shortcodes/blocks/list/block.editor.build.css dist/ten321/post-content-shortcodes/blocks/list/editor.css && cleancss -o dist/ten321/post-content-shortcodes/blocks/list/block.build.css dist/ten321/post-content-shortcodes/blocks/list/style.css",
    "webpack": "webpack",
    "uglify": "uglifyjs -o dist/ten321/post-content-shortcodes/blocks/content/block.min.js dist/ten321/post-content-shortcodes/blocks/content/block.js && uglifyjs -o dist/ten321/post-content-shortcodes/blocks/list/block.min.js dist/ten321/post-content-shortcodes/blocks/list/block.js",
    "build:css": "npm run scss && npm run cssmin",
    "build:js": "npm run webpack && npm run uglify",
    "build": "npm run build:css && npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cgrymala@github.com/cgrymala/post-content-shortcodes.git"
  },
