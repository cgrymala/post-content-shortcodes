  },
  "homepage": "https://github.com/cgrymala/post-content-shortcodes#readme",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "clean-css-cli": "^4.3.0",
    "concat": "^1.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "node-sass": "^4.14.1",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
